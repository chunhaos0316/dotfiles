[core]
	pager = cat
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[alias]
    l = log --oneline --graph --decorate --color=always
    la = log --oneline --graph --decorate --all --color=always
    d = diff --color=always
    dn = diff --name-only --color=always
